<h1>Create new Post</h1>
<%= form_for @post do |f| %>
    <%= f.label :title %> <br>
    <%= f.text_field :title %> <br>
    
    <%= f.label :content %> <br>
    <%= f.text_area :content, cols: "60", rows: "2" %> <br>

    <%= f.label :image, "Image:" %> 
    <%= f.file_field :image %> <br>

    <%= f.submit "Submit" %>
<% end %>